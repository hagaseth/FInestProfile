    xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
    xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
    xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2"
    xmlns:ext="urn:oasis:names:specification:ubl:schema:xsd:CommonExtensionComponents-2"
    xmlns:xades="http://uri.etsi.org/01903/v1.3.2#" xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
    xmlns:sbc="urn:oasis:names:specification:ubl:schema:xsd:SignatureBasicComponents-2"
    xmlns:ns0="urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents-2"
    xmlns:ns1="http://uri.etsi.org/01903/v1.4.1#"
    xmlns:ns2="urn:oasis:names:specification:ubl:schema:xsd:TransportServiceDescriptionCoreProfile-2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="urn:oasis:names:specification:ubl:schema:xsd:TransportServiceDescriptionCoreProfile-2 file:/T:/Prosjekt/P2801/280158%20FInest/04%20Analyser%20og%20draft/WP7/D7.4%20Prototype%20implementation/datamodel-FInest-19-10-2012/prototype/FinestProfile-12112012/TSD/TSDCoreProfile/xsd/maindoc/UBL-TSD-FInestProfile.xsd">
    <cbc:UBLVersionID>2.1</cbc:UBLVersionID>
    <cbc:ProfileID>FInest</cbc:ProfileID>
    <!-- Reply from ECM containing service description -->
    <cbc:ID>TSD1-1</cbc:ID>
    <cbc:IssueDate>2012-03-12</cbc:IssueDate>
    <cbc:IssueTime>01:01:01.001</cbc:IssueTime>
    <!-- This TSD is a description of a general Truck service at the west coast of Norway performed by Transferd AS -->
    <!-- This TSD is sent by the LSP as a response to a TSDrequest received from the LSC -->
    <!-- The TSDrequest contains the exact description of the transport demand -->
    <cbc:ServiceName>Truck transport</cbc:ServiceName>
    <!-- ResponseCode, ex. No relevant TSD is received for a TSDrequest -->
    <cbc:ResponseCode>ResponseCode0</cbc:ResponseCode>
    <cac:SenderParty>
        <cac:PartyIdentification>
            <cbc:ID>3743</cbc:ID>
        </cac:PartyIdentification>
        <cac:PartyName>
            <cbc:Name>ECM</cbc:Name>
        </cac:PartyName>
    </cac:SenderParty>
    <cac:ReceiverParty>
        <cac:PartyIdentification>
            <cbc:ID>200</cbc:ID>
        </cac:PartyIdentification>
        <cac:PartyName>
            <cbc:Name>TPM</cbc:Name>
        </cac:PartyName>
    </cac:ReceiverParty>
    <cac:TransportServiceDescriptionRequestDocumentReference>
        <cbc:ID>TSDreq1-1</cbc:ID>
    </cac:TransportServiceDescriptionRequestDocumentReference>
    <cac:TransportServiceProviderParty>
        <!-- Truck transporter Transferd AS -->
        <cac:PartyIdentification>
            <cbc:ID>100</cbc:ID>
        </cac:PartyIdentification>
        <cac:PartyName>
            <cbc:Name>Transferd AS</cbc:Name>
        </cac:PartyName>
        <cac:PostalAddress>
            <cbc:Postbox>204</cbc:Postbox>
            <cbc:CityName>Forde</cbc:CityName>
            <cbc:PostalZone>6802</cbc:PostalZone>
            <cac:Country>
                <cbc:IdentificationCode>NO</cbc:IdentificationCode>
                <cbc:Name>Norway</cbc:Name>
            </cac:Country>
        </cac:PostalAddress>
        <cac:Contact>
            <cbc:ID>ID2</cbc:ID>
            <cbc:Name>Leif Kåre Nogva</cbc:Name>
            <cbc:Telephone>70133022</cbc:Telephone>
            <cbc:ElectronicMail>leif.kaare.nogva@transferd.no</cbc:ElectronicMail>
        </cac:Contact>
    </cac:TransportServiceProviderParty>
    <cac:ServiceChargePaymentTerms>
        <!-- Note is used to hold the Agreed incoterms, if relevant -->
        <cbc:Note>FOB</cbc:Note>
        <!--  COST * (€ / TEU**) : 300 . 2 containers = 600 euro ** 1 TEU (=1/2 40ft Container) = 8 000 Kg !-->
        <cbc:Amount currencyID="Euro">600</cbc:Amount>
        <cbc:PaymentDueDate>2012-06-01</cbc:PaymentDueDate>
    </cac:ServiceChargePaymentTerms>
    <!-- the period where these transport service is valid, for instance for a scheduled service -->
    <cac:ValidityPeriod>
        <cbc:StartDate>2012-01-01</cbc:StartDate>
        <cbc:StartTime>01:01:01.001</cbc:StartTime>
        <cbc:EndDate>2013-12-31</cbc:EndDate>
        <cbc:EndTime>01:01:01.001</cbc:EndTime>
    </cac:ValidityPeriod>
    <cac:TransportationService>
        <!-- TransportServiceCode: 3 Transport       -->
        <cbc:TransportServiceCode>3</cbc:TransportServiceCode>
        <cbc:TransportationServiceDescription>Truck transport at the West Coast of Norway</cbc:TransportationServiceDescription>
        <cbc:Name>Truck transport</cbc:Name>
        <cbc:SequenceNumeric>1</cbc:SequenceNumeric>
        <cac:TransportEquipment>
            <cbc:ID>ID11</cbc:ID>
            <cbc:Description>Container</cbc:Description>
            <!--CN is Container-->
            <!-- TransportEquipmentTypeCode	UN/CEFACT 8053 Equipment type code qualifier:-->
            <cbc:TransportEquipmentTypeCode>CN</cbc:TransportEquipmentTypeCode>
            <!-- 22G0 	GENERAL PURPOSE CONT. Find other attribute for this -->
            <!--            <cbc:SizeTypeCode>22G0</cbc:SizeTypeCode> -->
            <cbc:GrossWeightMeasure unitCode="kg">20000</cbc:GrossWeightMeasure>
            <!-- m3 -->
            <cbc:GrossVolumeMeasure unitCode="cubic metre">30</cbc:GrossVolumeMeasure>
        </cac:TransportEquipment>
        <cac:CommodityClassification>
            <!-- CargoTypeCode	UN/CEFACT 7085 Nature of cargo, coded
                                -->
            <!--12 is General Cargo-->
            <cbc:CargoTypeCode>12</cbc:CargoTypeCode>
        </cac:CommodityClassification>
        <cac:CommodityClassification>
            <!-- CargoTypeCode	UN/CEFACT 7085 Nature of cargo, coded
                                -->
            <!--9	Containerized-->
            <cbc:CargoTypeCode>9</cbc:CargoTypeCode>
        </cac:CommodityClassification>
        <cac:ShipmentStage>
            <cbc:ID>ID15</cbc:ID>
            <!-- Transport mode code: 3 Road transport  !-->
            <cbc:TransportModeCode>3</cbc:TransportModeCode>
            <cac:TransportMeans>
                <cbc:JourneyID>JourneyID0</cbc:JourneyID>
                <!-- 37	Truck, container -->
                <cbc:TransportMeansTypeCode>37</cbc:TransportMeansTypeCode>
            </cac:TransportMeans>
        </cac:ShipmentStage>
        <!-- TransportEvent: Not given in this TSD since this is a reply to a request from
        the LSC. The actual planned events are negotiated in the TEP.-->
        <cac:EstimatedDurationPeriod>
            <cbc:StartDate>2012-05-06</cbc:StartDate>
            <cbc:StartTime>10:00:00.00</cbc:StartTime>
            <cbc:EndDate>2012-05-07</cbc:EndDate>
            <cbc:EndTime>14:00:00.00</cbc:EndTime>
        </cac:EstimatedDurationPeriod>
    </cac:TransportationService>
</ns2:TransportServiceDescriptionCoreProfile>
