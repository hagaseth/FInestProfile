         <xsd:element ref="cbc:ReferenceTime" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <ccts:Component>
                     <ccts:ComponentType>BBIE</ccts:ComponentType>
                     <ccts:DictionaryEntryName>Transport Schedule. Reference Time.
                        Time</ccts:DictionaryEntryName>
                     <ccts:Definition>The reference time for the transport schedule
                        status.</ccts:Definition>
