            </xsd:annotation>
         </xsd:element>
         <xsd:element ref="cbc:SerialID" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>
                  <ccts:Component>
                     <ccts:ComponentType>BBIE</ccts:ComponentType>
                     <ccts:DictionaryEntryName>Item Instance. Serial Identifier.
                        Identifier</ccts:DictionaryEntryName>
