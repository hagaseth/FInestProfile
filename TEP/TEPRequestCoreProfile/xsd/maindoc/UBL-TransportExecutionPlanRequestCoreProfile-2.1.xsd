<?xml version="1.0" encoding="UTF-8"?>
<!--
Core Profile of the TransportExecutionPlanRequest (TEP Request)
-->
<xsd:schema xmlns="urn:oasis:names:specification:ubl:schema:xsd:TransportExecutionPlanRequestCoreProfile-2"
